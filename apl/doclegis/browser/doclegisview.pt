<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="apl.doclegis">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main"
                        tal:define="testview view/test">

            
			<table id="doclegisTable">
				<thead>
					<th>Titre</th>
					<th>Type</th>
					<th>Date</th>
				</thead>
				<tr tal:repeat="doclegis view/get_doclegis">
					<td tal:content="doclegis/title">title</td>
					<td tal:content="doclegis/document_type">type</td>
					<td tal:content="doclegis/commune">date</td>
				</tr>
			</table>
        </tal:main-macro>
    </div>
</body>
</html>
